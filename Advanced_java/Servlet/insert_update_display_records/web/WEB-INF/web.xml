<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MyServlet2</servlet-name>
        <servlet-class>MyServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contactPage</servlet-name>
        <servlet-class>contactPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayStudentInfo</servlet-name>
        <servlet-class>DisplayStudentInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectIndividualData</servlet-name>
        <servlet-class>SelectIndividualData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>data</servlet-name>
        <servlet-class>data</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet2</servlet-name>
        <url-pattern>/MyServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contactPage</servlet-name>
        <url-pattern>/contactPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayStudentInfo</servlet-name>
        <url-pattern>/DisplayStudentInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectIndividualData</servlet-name>
        <url-pattern>/SelectIndividualData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>data</servlet-name>
        <url-pattern>/data</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
