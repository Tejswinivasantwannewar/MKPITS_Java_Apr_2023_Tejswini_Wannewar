<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Servlets.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>connectionServlet</servlet-name>
        <servlet-class>Servlets.connectionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DepositServlet</servlet-name>
        <servlet-class>Servlets.DepositServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionTrackingServlet</servlet-name>
        <servlet-class>Servlets.SessionTrackingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deposit_Servlet</servlet-name>
        <servlet-class>Servlets.Deposit_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>connectionServlet</servlet-name>
        <url-pattern>/connectionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DepositServlet</servlet-name>
        <url-pattern>/DepositServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionTrackingServlet</servlet-name>
        <url-pattern>/SessionTrackingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deposit_Servlet</servlet-name>
        <url-pattern>/Deposit_Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
